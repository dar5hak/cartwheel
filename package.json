{
  "name": "cartwheel",
  "version": "1.0.0",
  "main": "dist/cartwheel.cjs.js",
  "module": "dist/cartwheel.esm.js",
  "browser": "dist/cartwheel.umd.js",
  "devDependencies": {
    "ava": "^3.13.0",
    "esm": "^3.2.25",
    "rollup": "^2.30.0",
    "xo": "^0.34.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "xo && ava",
    "pretest": "npm run build",
    "prepublish": "npm run build"
  },
  "files": [
    "dist"
  ],
  "xo": {
    "extensions": [
      ".mjs"
    ]
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
